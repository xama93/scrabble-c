#include "Jugador.h"
#include <iostream>

using namespace std;

Jugador::Jugador()
{
}

Rack Jugador::getRack()//const
{
  return(rack);
}

void Jugador::setRack(Rack r)
{
  this->rack=r;
}

int Jugador::getPuntuacion()const
{
  return(puntuacion);
}

void Jugador::setPuntuacion(int puntuacion)
{
  this->puntuacion = puntuacion;
}